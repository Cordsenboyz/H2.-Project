        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkoleGUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="458" Width="808">
    <DockPanel>
        <WrapPanel Orientation="Vertical" DockPanel.Dock="Left">
            <Button Content="Person" Height="40" Width="90" Margin="20, 20, 20, 10" Click="MenuPerson_Click" />
            <Button Content="Elev" Height="40" Width="90" Margin="20, 10, 20, 10" Click="MenuElev_Click" />
            <Button Content="Lærer" Height="40" Width="90" Margin="20, 10, 20, 10" Click="MenuLærer_Click" />
        </WrapPanel>
        <local:ContentArea x:Name="contentArea">
        </local:ContentArea>
    </DockPanel>
</Window>